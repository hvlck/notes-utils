{
    "manifest_version": 2,
    "name": "notes-utils",
    "description": "personal note utilities web extension",
    "version": "0.0.1",
    "icons": {
        "64": "icons/icon.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": {
            "64": "icons/icon.png"
        },
        "default_popup": "browserAction/index.html",
        "default_title": "web"
    },
    "options_ui": {
        "page": "options/index.html"
    }
}